<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Registres_Acces.SelectorAfegirEliminar">



    <include
        android:id="@+id/idToolBar"
        layout="@layout/toolbar"
        />

    <LinearLayout
       android:id="@+id/idLinearLayoutSlector"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:orientation="horizontal">

    </LinearLayout>


    <Button
        android:id="@+id/idButtonAfegirVideos"
        android:layout_width="141dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_centerVertical="true"
        android:background="@drawable/buttons"
        android:layout_above="@+id/idLinearLayoutSlector"
        android:layout_margin="50dp"
        android:text="Afegir Vídeos"
        android:textColor="@color/white"

        >


    </Button>

    <Button
        android:id="@+id/idButtonEliminarVideos"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/idButtonAfegirVideos"
        android:layout_centerInParent="true"
        android:layout_marginTop="50dp"
        android:background="@drawable/buttons"
        android:text="Eliminar Vídeos"
        android:textColor="@color/white"

        ></Button>






</RelativeLayout>